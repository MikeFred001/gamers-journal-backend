    "$id": "http://example.com/userUpdate.schema.json",
    "type": "object",
    "properties": {
        "password": {
            "type": "string",
            "minLength": 5,
            "maxLength": 30
        },
        "firstName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "lastName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "email": {
            "type": "string",
            "format": "email",
            "minLength": 6,
            "maxLength": 60
        }
    },
    "additionalProperties": false,
    "required": []
}